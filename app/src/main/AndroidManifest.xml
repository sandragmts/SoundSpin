<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.soundspin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".songs"
            android:label="@string/songs"
            android:parentActivityName="com.example.android.soundspin.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>


        <activity
            android:name=".artists"
            android:label="@string/singer"
            android:parentActivityName="com.example.android.soundspin.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".albums"
            android:label="@string/album"
            android:parentActivityName="com.example.android.soundspin.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".playlists"
            android:label="@string/playlist"
            android:parentActivityName="com.example.android.soundspin.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".moanasongs"
            android:label="MOANA"
            android:parentActivityName="com.example.android.soundspin.moanadisc">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".moanadisc" />
        </activity>


        <activity
            android:name=".moanadisc"
            android:parentActivityName="com.example.android.soundspin.songs">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".songs" />
        </activity>


        <activity
            android:name=".frozensongs"
            android:label="FROZEN"
            android:parentActivityName="com.example.android.soundspin.frozendisc">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".frozendisc" />
        </activity>


        <activity
            android:name=".frozendisc"
            android:parentActivityName="com.example.android.soundspin.songs">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".songs" />
        </activity>

        <activity
            android:name=".aladdinsongs"
            android:label="ALADDIN"
            android:parentActivityName="com.example.android.soundspin.songs">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".songs" />
        </activity>

        <activity
            android:name=".aladdindisc"
            android:parentActivityName="com.example.android.soundspin.songs">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".songs" />
        </activity>


    </application>

</manifest>