<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.soundspin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".songs"
            android:label="@string/songs" />
        <activity
            android:name=".artists"
            android:label="@string/singer" />
        <activity
            android:name=".albums"
            android:label="@string/album" />
        <activity
            android:name=".playlists"
            android:label="@string/playlist" />

        <activity
            android:name=".moanasongs"
            android:label="MOANA" />

        <activity android:name=".moanadisc"/>

        <activity
            android:name=".frozensongs"
            android:label="FROZEN" />

        <activity android:name=".frozendisc"/>

        <activity
            android:name=".aladdinsongs"
            android:label="ALADDIN" />

        <activity android:name=".aladdindisc"/>


    </application>

</manifest>